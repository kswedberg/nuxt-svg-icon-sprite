{
  "name": "nuxt-svg-icon-sprite",
  "version": "2.0.1",
  "description": "Generate SVG symbol sprites for icons and illustrations.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dulnan/nuxt-svg-icon-sprite.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./processors": "./dist/processors.mjs"
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ],
      "processors": [
        "./dist/processors.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground --trace-warnings",
    "dev:debug": "nuxi dev playground --trace-warnings --inspect",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground && nuxi prepare ./test/e2e/fixtures/basic",
    "dev:serve": "node playground/.output/server/index.mjs",
    "dev:fixtures:basic": "nuxi dev ./test/e2e/fixtures/basic",
    "docs:dev": "vitepress dev docs --port 5000",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs --port 5000",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "typecheck": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "test": "TZ=utc vitest",
    "test:debug": "TZ=utc vitest --disable-console-intercept --no-file-parallelism",
    "test:ci": "TZ=utc vitest run",
    "test:coverage": "TZ=utc vitest run --coverage"
  },
  "dependencies": {
    "node-html-parser": "^7.0.1"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.3.0",
    "@nuxt/kit": "^3.17.1",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.1",
    "@nuxt/test-utils": "^3.17.2",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "happy-dom": "^17.4.6",
    "nuxt": "^3.17.1",
    "playwright-core": "^1.52.0",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "vitest": "^3.1.2",
    "vitest-environment-nuxt": "^1.0.1",
    "vue-tsc": "^2.2.8"
  }
}
